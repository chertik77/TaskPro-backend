          "201": { "$ref": "#/components/responses/ColumnResponse" },
          "400": { "$ref": "#/components/responses/ColumnBadRequest" },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "404": { "$ref": "#/components/responses/NotFound" }
        }
      }
    },

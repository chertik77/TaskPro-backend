{
  "openapi": "3.1.0",
  "info": {
    "version": "2.2.0",
    "title": "Task Pro API",
    "description": "The Task Pro API provides endpoints to manage projects and user assignments, ensuring secure and efficient request handling.",
    "contact": {
      "name": "TaskPro",
      "url": "https://www.taskpro.online"
    }
  },
  "servers": [
    { "url": "http://localhost:8000/api - Development" },
    { "url": "https://api.taskpro.online - Production" }
  ],
  "paths": {
    "/auth/signup": {
      "post": {
        "tags": ["Auth"],
        "summary": "Create a new user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "name": "John Doe",
                "email": "user@example.com",
                "password": "qwerty123"
              }
            }
          }
        },
        "responses": {
          "201": { "$ref": "#/components/responses/AuthResponse" },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "message": {
                    "email": "Email is invalid",
                    "password": "Password must be at least 8 characters",
                    "name": "Name must be at least 2 characters"
                  }
                }
              }
            }
          },
          "409": { "$ref": "#/components/responses/Conflict" }
        }
      }
    },
    "/auth/signin": {
      "post": {
        "tags": ["Auth"],
        "summary": "Signin a user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "email": "user@example.com",
                "password": "qwerty123"
              }
            }
          }
        },
        "responses": {
          "201": { "$ref": "#/components/responses/AuthResponse" },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "message": {
                    "email": "Email is invalid",
                    "password": "Password must be at least 8 characters"
                  }
                }
              }
            }
          },
          "401": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 401,
                  "message": "Email or password invalid"
                }
              }
            }
          }
        }
      }
    },
    "/auth/google": {
      "post": {
        "tags": ["Auth"],
        "summary": "Signin a user with google",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "code": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjY2NzE3NDNkYjg1Yzk2NDUzZTA0YTY4NCIsInNpZCI6IjY2NzE3OWE0YTgxZmI1MjIzOTg0NmY2MSIsImlhdCI6MTcxODcxMjc0MCwiZXhwIjoxNzE5MzE3NTQwfQ.B8htZY5kU0kvX2UYsEN0u7ZUPVxTKfCs5ASAocTc-P4"
              }
            }
          }
        },
        "responses": {
          "201": { "$ref": "#/components/responses/GoogleResponse" },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "message": {
                    "code": "Code must be a string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth/tokens": {
      "post": {
        "tags": ["Auth"],
        "summary": "Get new access and refresh tokens",
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "refreshToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjY2NzE3NDNkYjg1Yzk2NDUzZTA0YTY4NCIsInNpZCI6IjY2NzE3OWE0YTgxZmI1MjIzOTg0NmY2MSIsImlhdCI6MTcxODcxMjc0MCwiZXhwIjoxNzE5MzE3NTQwfQ.B8htZY5kU0kvX2UYsEN0u7ZUPVxTKfCs5ASAocTc-P4"
              }
            }
          }
        },
        "responses": {
          "200": { "$ref": "#/components/responses/TokensResponse" },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "message": {
                    "refreshToken": "refreshToken must be a string"
                  }
                }
              }
            }
          },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "403": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 403,
                  "message": "Forbidden"
                }
              }
            }
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "tags": ["Auth"],
        "summary": "Logout a user",
        "security": [{ "Bearer": [] }],
        "responses": {
          "204": { "$ref": "#/components/responses/NoContent" },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/user/me": {
      "get": {
        "tags": ["User"],
        "summary": "Get current user",
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": { "$ref": "#/components/responses/UserResponse" },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/user/help": {
      "post": {
        "tags": ["User"],
        "summary": "Send email need help",
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": { "email": "user@example.com", "comment": "Need help" }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": { "example": { "message": "Email sent" } }
            }
          },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "message": {
                    "email": "Email is required",
                    "comment": "Comment is required"
                  }
                }
              }
            }
          },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/user": {
      "put": {
        "tags": ["User"],
        "summary": "Edit user profile",
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": { "example": { "password": "123456789" } },
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "avatar": { "type": "string", "format": "binary" }
                }
              }
            }
          }
        },
        "responses": {
          "200": { "$ref": "#/components/responses/UserResponse" },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "message": {
                    "email": "Email is invalid",
                    "name": "Name must be at least 2 characters"
                  }
                }
              }
            }
          },
          "406": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 406,
                  "message": "File must have .jpeg or .png extension"
                }
              }
            }
          },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "409": { "$ref": "#/components/responses/Conflict" }
        }
      }
    },
    "/user/theme": {
      "put": {
        "tags": ["User"],
        "summary": "Switch user theme",
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": { "example": { "theme": "light" } }
          }
        },
        "responses": {
          "200": { "$ref": "#/components/responses/UserResponse" },
          "400": {
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 400,
                  "errors": {
                    "theme": "Theme must be one of the following: light, dark, violet"
                  }
                }
              }
            }
          },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/board": {
      "get": {
        "tags": ["Board"],
        "summary": "Get all boards",
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": { "$ref": "#/components/responses/AllBoardsResponse" },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      },
      "post": {
        "tags": ["Board"],
        "summary": "Add new board",
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "title": "My office",
                "icon": "lightning",
                "background": "blue-ball"
              }
            }
          }
        },
        "responses": {
          "201": { "$ref": "#/components/responses/BoardResponse" },
          "400": { "$ref": "#/components/responses/BoardBadRequest" },
          "401": { "$ref": "#/components/responses/Unauthorized" }
        }
      }
    },
    "/board/{boardId}": {
      "get": {
        "tags": ["Board"],
        "summary": "Get board by id",
        "parameters": [{ "in": "path", "name": "boardId", "required": true }],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": { "$ref": "#/components/responses/BoardByIdResponse" },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "404": { "$ref": "#/components/responses/NotFound" }
        }
      },
      "put": {
        "tags": ["Board"],
        "summary": "Edit board by id",
        "parameters": [{ "in": "path", "name": "boardId", "required": true }],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": { "example": { "title": "My office" } }
          }
        },
        "responses": {
          "200": { "$ref": "#/components/responses/BoardResponse" },
          "400": { "$ref": "#/components/responses/BoardBadRequest" },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "404": { "$ref": "#/components/responses/NotFound" }
        }
      },
      "delete": {
        "tags": ["Board"],
        "summary": "Delete board by id",
        "parameters": [{ "in": "path", "name": "boardId", "required": true }],
        "security": [{ "Bearer": [] }],
        "responses": {
          "204": { "$ref": "#/components/responses/NoContent" },
          "401": { "$ref": "#/components/responses/Unauthorized" },
          "404": { "$ref": "#/components/responses/NotFound" }
        }
      }
    },
    "/column/{boardId}": {
      "post": {
        "tags": ["Column"],
